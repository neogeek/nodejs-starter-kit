{
  "name": "@neogeek/nodejs-starter-kit",
  "version": "0.0.1",
  "description": "This repo is a starter kit for working with Node.js, Handlebars, Sass, and Babel.",
  "engines": {
    "node": ">=12.0"
  },
  "main": "web.js",
  "license": "MIT",
  "dependencies": {
